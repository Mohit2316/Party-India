<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" >
  <div class="modal-dialog" style="background-color:white;border-radius:6px;width:25vw;">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="exampleModalLabel" style="text-align:center">Create Your Event</h4>
      </div>
      <div class="modal-body" style="text-align:center">
        
          <div class="form-group">
            <a role="button" href="/events/new" class="btn btn-primary" style="width:15vw">Create New Event Manually</a>
          </div>
          
          <div class="form-group">
            <button class="btn btn-primary" style="width:15vw"><i class="fa fa-facebook" ></i> Import from Facebook</button>
          </div>
          <%= form_for(@event , url:{action:"create", controller:"events"}) do |f|%>
          <div class="form-group" >
            <label for="facebook-import" class="control-label">Import from facebook event link</label><br/>
            <span></span>
            <%= f.text_field :club_url, id: "facebook-import", class:"form-control", placeholder: "Paste url here",style: "width:15vw;display:inline-block;"%>
            
            <%= f.hidden_field :source, :value => "facebook" %>
            <%= f.submit "Import" ,class: "btn btn-primary"  %>
            <span></span>
          </div>
          <% end%>
      </div>
      
    </div>
  </div>
</div>
	
